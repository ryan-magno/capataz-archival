{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "capatazarchivaladf"
		},
		"CapatazArchiveDBLinkedService_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'CapatazArchiveDBLinkedService'"
		},
		"CapatazLiveADFLinkedService_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'CapatazLiveADFLinkedService'"
		},
		"CapatazArchiveDBLinkedService_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "capatazserver.database.windows.net"
		},
		"CapatazArchiveDBLinkedService_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "ArchivalDB-Simulation"
		},
		"CapatazArchiveDBLinkedService_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sqladmin"
		},
		"CapatazLiveADFLinkedService_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "capatazserver.database.windows.net"
		},
		"CapatazLiveADFLinkedService_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "New-Capataz-Staging"
		},
		"CapatazLiveADFLinkedService_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sqladmin"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "capatazarchivaladf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"CapatazArchiveDBLinkedService_password": {
						"value": "[parameters('CapatazArchiveDBLinkedService_password')]"
					},
					"CapatazLiveADFLinkedService_password": {
						"value": "[parameters('CapatazLiveADFLinkedService_password')]"
					},
					"CapatazArchiveDBLinkedService_properties_typeProperties_server": {
						"value": "[parameters('CapatazArchiveDBLinkedService_properties_typeProperties_server')]"
					},
					"CapatazArchiveDBLinkedService_properties_typeProperties_database": {
						"value": "[parameters('CapatazArchiveDBLinkedService_properties_typeProperties_database')]"
					},
					"CapatazArchiveDBLinkedService_properties_typeProperties_userName": {
						"value": "[parameters('CapatazArchiveDBLinkedService_properties_typeProperties_userName')]"
					},
					"CapatazLiveADFLinkedService_properties_typeProperties_server": {
						"value": "[parameters('CapatazLiveADFLinkedService_properties_typeProperties_server')]"
					},
					"CapatazLiveADFLinkedService_properties_typeProperties_database": {
						"value": "[parameters('CapatazLiveADFLinkedService_properties_typeProperties_database')]"
					},
					"CapatazLiveADFLinkedService_properties_typeProperties_userName": {
						"value": "[parameters('CapatazLiveADFLinkedService_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}